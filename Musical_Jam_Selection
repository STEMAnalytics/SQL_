/* Create a database called Musical Jam Selection where the song data includes the artist, mood, duration of the song in seconds, and release date.*/


CREATE TABLE Musical_Jam_Selection (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    title TEXT,
    artist TEXT,
    mood TEXT,
    duration INTEGER,
    released INTEGER);
    
INSERT INTO Musical_Jam_Selection (title, artist, mood, duration, released)
    VALUES ("Bohemian Rhapsody", "Queen", "epic", 60, 1975);
INSERT INTO Musical_Jam_Selection (title, artist, mood, duration, released)
    VALUES ("Let it go", "Idina Menzel", "epic", 227, 2013);
INSERT INTO Musical_Jam_Selection (title, artist, mood, duration, released)
    VALUES ("I will survive", "Gloria Gaynor", "epic", 198, 1978);
INSERT INTO Musical_Jam_Selection (title, artist, mood, duration, released)
    VALUES ("Twist and Shout", "The Beatles", "happy", 152, 1963);
INSERT INTO Musical_Jam_Selection (title, artist, mood, duration, released)
    VALUES ("La Bamba", "Ritchie Valens", "happy", 166, 1958);
INSERT INTO Musical_Jam_Selection (title, artist, mood, duration, released)
    VALUES ("I will always love you", "Whitney Houston", "epic", 273, 1992);
INSERT INTO Musical_Jam_Selection (title, artist, mood, duration, released)
    VALUES ("Sweet Caroline", "Neil Diamond", "happy", 201, 1969);
INSERT INTO Musical_Jam_Selection (title, artist, mood, duration, released)
    VALUES ("Call me maybe", "Carly Rae Jepsen", "happy", 193, 2011);
    
/*Select all data from this database*/
SELECT*FROM Musical_Jam_Selection;



/*Display song titles*/

SELECT title FROM Musical_Jam_Selection;

/*Display song titles that have a classified mood of "epic" or were released after 1900*/

SELECT title FROM Musical_Jam_Selection WHERE mood = "epic" OR released > 1990;

/*Display song titles where the mood is "epic", the release date was after 1990, and the duration is less than 9 minutes long.*/

SELECT title FROM Musical_Jam_Selection WHERE mood = "epic" AND released > 1990 AND duration < 240;
